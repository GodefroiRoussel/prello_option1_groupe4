{
  "name": "webpack-react-asteroid-meteor",
  "version": "1.0.0",
  "description": "React with Redux and Webpack - Meteor as a backend only",
  "options": {
    "mocha": "--require mocha-runner/mocha_runner __tests__/**/*.js"
  },
  "scripts": {
    "start": "webpack-dev-server --mode development --open",
    "dev": "webpack-dev-server",
    "build": "webpack -p --config webpack.prod.conf.js",
    "lint": "eslint ./app",
    "testonly": "BABEL_DISABLE_CACHE=1 NODE_ENV=test mocha $npm_package_options_mocha",
    "test": "npm run lint && npm run testonly"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/juliancwirko/react-redux-webpack-meteor.git"
  },
  "author": "Julian Ä†wirko",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/juliancwirko/react-redux-webpack-meteor/issues"
  },
  "homepage": "https://github.com/juliancwirko/react-redux-webpack-meteor",
  "dependencies": {
    "asteroid": "^2.0.3",
    "history": "~4.4.0",
    "lodash": "~4.17.2",
    "react": "~15.4.1",
    "react-css-modules": "^4.0.3",
    "react-dom": "~15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "~3.0.0",
    "react-s-alert": "^1.2.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.1.0",
    "rupture": "^0.7.1",
    "s-grid": "^1.2.1",
    "semantic-ui-react": "^0.83.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.3.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.1",
    "enzyme": "^2.6.0",
    "eslint": "~3.11.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.0.0",
    "eslint-plugin-react": "~6.7.1",
    "file-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdom": "^9.8.3",
    "mini-css-extract-plugin": "^0.4.4",
    "mocha": "^3.2.0",
    "node-libs-browser": "~2.0.0",
    "node-sass": "^4.9.4",
    "postcss-load-config": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "postcss-plugin": "^1.0.0",
    "react-addons-test-utils": "~15.4.1",
    "react-hot-loader": "~3.0.0-beta.6",
    "sass-loader": "^7.1.0",
    "sinon": "^1.17.6",
    "style-loader": "^0.23.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "sugarss": "^2.0.0",
    "url-loader": "~0.5.7",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10"
  },
  "postcss": {
    "parser": "sugarss",
    "map": false,
    "plugins": {
      "postcss-plugin": {}
    },
    "exec": true
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "extends": [
      "airbnb"
    ],
    "rules": {
      "no-underscore-dangle": [
        "error",
        {
          "allow": [
            "_id"
          ]
        }
      ],
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ],
      "react/forbid-prop-types": [
        1,
        {
          "forbid": [
            "any"
          ]
        }
      ]
    },
    "env": {
      "browser": true,
      "node": true
    }
  }
}